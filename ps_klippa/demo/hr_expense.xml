<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="expense_demo1" model="hr.expense">
        <field name="name">an expense</field>
        <field name="employee_id" ref="hr.employee_qdp" />
        <field
            name="analytic_distribution"
            eval="{str(ref('analytic.analytic_our_super_product')): 100}"
        />
        <field name="product_id" ref="hr_expense.product_product_no_cost" />
        <field name="unit_amount">42</field>
        <field name="total_amount">42</field>
        <field name="product_uom_id" ref="uom.product_uom_km" />
    </record>
